  <!-- CONTACT SECTION START-->
  <section id="contact" class="contact">
    <h2><span>Our</span> Contact</h2>
    <div class="row">
      <iframe
        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d31687.94130543058!2d107.5725502743164!3d-6.891479999999991!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x2e68e65767c9b183%3A0x2478e3dcdce37961!2sInstitut%20Teknologi%20Bandung!5e0!3m2!1sid!2sid!4v1712984704783!5m2!1sid!2sid"
        allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade" class="map"></iframe>
      <form action="" id='contact-form'>
        <div class="input-group">
          <i class="bi bi-person"></i>
          <input type="text" placeholder="Nama" name="user_name">
        </div>
        <div class="input-group">
          <i class="bi bi-envelope"></i>
          <input type="email" placeholder="Email" name="user_email">
        </div>
        <div class="input-group">
          <i class="bi bi-telephone"></i>
          <input type="text" placeholder="No hp" name="contact_number">
        </div>
        <div class="msg">
          <label>Message</label>
          <textarea name="message"></textarea>
        </div>
        <input type="submit" class="btn" value="Send">
      </form>
    </div>
  </section>
  <!-- CONTACT SECTION END-->
